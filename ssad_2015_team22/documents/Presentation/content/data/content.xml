    <object id="4_30863873" type="text" x="-64623.014170505856" y="1687.5828203213186" r="-1.3444796819418078e-14" s="6.334511718741431" class="body">
      <height>230.4</height>
      <width>390.9237784827681</width>
      <p listType="bullet" indent="1">
        <text color="1657497" weight="bold" style="italic"><![CDATA[Student can take tests on various modules.]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="1657497" weight="bold" style="italic"><![CDATA[Student can check scores and correct answers after the test.]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="1657497" weight="bold" style="italic"><![CDATA[Explanations about the correct answers are also available.]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text color="1657497" weight="bold" style="italic"><![CDATA[Student can see his past performances history]]></text>
      </p>
      <layout>
